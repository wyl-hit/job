<script type="text/javascript">
/*
function: channel homepage
modified: 20120924
coding:austinjin
*/
var gBossZoneHandler = new bossZoneHandler();
var gImage;
var registerZone2 = gBossZoneHandler.registerZone2;

if(document.addEventListener)
    document.addEventListener("click", registerZone2, false);
else if(document.attachEvent)
    document.attachEvent("onclick", registerZone2);

function bossZoneHandler()
{
    var sFlag=false;
	this.registerZone2 = function (ev,clickType)
	{
	    var loopTryNum = 10;
	    var bossID = 1337; 
		  
	    try
		{		
		    //qq
			var a=document.cookie.match(new RegExp('(^|)o_cookie=([^;]*)(;|$)'));
			var iQQ=(a==null?"":unescape(a[2]));
			var purl='';
			var zoneId='';
			
			if(typeof clickType == 'undefined')
			{
			    if(sFlag) return true;
		        var ev = window.event || ev;
		        var et = ev.srcElement || ev.target;
			    var type=et.tagName;
		        if (type != "A" && type != "IMG" ) 
			    {
			        return true;
			    } 
			
			    if (type == "A")
			    {
			        purl = et.href;
			    }
			    else if (type == "IMG")
			    {
			        purl = et.parentNode.href;
			    }
			
			    //pseudo attr
			    for (var i=loopTryNum-1,tagNode=et;i>=0;i--,tagNode=tagNode.parentNode)
			    {
			        if(tagNode.attributes['bossZone'])
				    {
			            zoneId = tagNode.attributes['bossZone'].nodeValue;
				    } else if(tagNode.attributes['bosszone'])
				    {
				        zoneId = tagNode.attributes['bosszone'].nodeValue;
				    }
				    else if(tagNode.attributes['BossZone'])
				    {
				        zoneId = tagNode.attributes['BossZone'].nodeValue;
				    }
				    else if(tagNode.attributes['Bosszone'])
				    {
				        zoneId = tagNode.attributes['Bosszone'].nodeValue;
				    }

			        if(zoneId) break; 
			    }		
			    if(!zoneId) return;
			}
			else
			{
			    zoneId = ev.bossZone;
				purl = ev.url;
				sFlag = true;
				setTimeout(function (){sFlag = false;},200);
			}
			
			var localUrl = location.href;
			var site = localUrl.substring(7,localUrl.indexOf('.qq.com'));
			site = site.substr(site.lastIndexOf('.')+1);

			var iurl = 'http://btrace.qq.com/collect?sIp=&iQQ='+iQQ+'&sBiz=qq.com&sOp='+zoneId+'&iSta=&iTy='+bossID+'&iFlow=&sSite='+site+'&sUrl='+escape(purl)+'&sLocalUrl='+escape(location.href)+'&'+Math.random();
			gImage = new Image(1,1);
			gImage.src = iurl;
		} catch (e) {}
	}
}</script><!--[if !IE]>|xGv00|044b5aa9286e87debda646ce2f58e7cb<![endif]-->
<script type="text/javascript">
	function linkReplace(obj){
		$(obj).find("a").each(function(index){
			var defaultUrl =  $(this)[0].href,
				replaceUrl="",
				articleIdNum = 0,
				articleId ="",
				targetLink ="http://new.qq.com/omn/";
			var idIndex = defaultUrl.lastIndexOf("\/"),
				urlLength = defaultUrl.length,
				tt=-1;
			if(defaultUrl.lastIndexOf("\?")){
				tt = defaultUrl.lastIndexOf("\?");
			}
			//获取文章id
			if(tt>=0){ //后面还带有其他参数
				if(defaultUrl.indexOf(".htm")>=0){
					articleId = defaultUrl.substring(idIndex + 1, tt-4);
				}else if(defaultUrl.indexOf(".html")>=0){
					articleId = defaultUrl.substring(idIndex + 1, tt-5);
				}else{
					articleId = defaultUrl.substring(idIndex + 1, tt);
				}
			}else{ //后面没有其他参数
				if(defaultUrl.indexOf(".htm")>=0){
					articleId = defaultUrl.substring(idIndex + 1, urlLength-4);
				}else if(defaultUrl.indexOf(".html")>=0){
					articleId = defaultUrl.substring(idIndex + 1, urlLength-5);
				}else{
					articleId = defaultUrl.substring(idIndex + 1, urlLength);
				}
			}
			articleIdNum = parseInt(articleId.replace(/[^0-9]/ig,"")); //提取数字部分
			//通过id来拼链接
			if(articleId.length==16){
				
				if(String(articleIdNum).length<14 && String(articleIdNum).length>0){
					articleId = articleId.substring(0, articleId.length-2);
				}
				replaceUrl = targetLink + articleId+'.html';
			}else if(articleId.length>16){
				if(String(articleIdNum).length<14 && String(articleIdNum).length>0){
					articleId = articleId.substring(0, articleId.length);
				}
				replaceUrl = targetLink + articleId+'.html';
			}else{
				replaceUrl = defaultUrl;
			}
			$(this)[0].href = replaceUrl;
			
			//替换分享的链接
			if($(this).attr("id")){ 
				var idUrl = $(this).attr("id");
				var idShareUrl="";
				var index = idUrl.lastIndexOf("\/");
				var articleShareId = idUrl.substring(index + 1, idUrl.length);
				if(articleShareId.length>=14){
					if(String(parseInt(articleShareId)).length<14){
						articleShareId = articleShareId.substring(0, articleShareId.length-2);
					}
					idShareUrl = targetLink +articleShareId+'.html';
				}else{
					idShareUrl = idUrl;
				}
				$(this).attr("id",idShareUrl);
			}
		});
	}<!--[if !IE]>|xGv00|10db2d549f757a15ca66bed3def37808<![endif]-->
	linkReplace(".military");//军事模块
</script>
<script type="text/javascript">
    try {
        QosS.endCheck();
    } catch (e) {}
</script>

