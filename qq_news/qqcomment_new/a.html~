<script type="text/javascript">
	(function(){
		// 设置导航中的某一个为current
		var url = window.location.href,
			arr = [
				['roll' , 'navlinkRoll'],
				['world' , 'navlinkWorld'],
				['society' , 'navlinkSociety'],
				['photo' , 'navlinkPhoto'], // 其他导航
				['v' , 'navlinkVideo'], // 视频，使用是v.qq.com的导航
				['military' , 'navlinkMil'],
				['view' , 'navlinkView'], // 无导航
				['history' , 'navlinkHistory'], // 其他导航
				['cul' , 'navlinkWh'], // 文化，其他导航
				['gy' , 'navlinkGuyu'], // 谷雨，其他导航
				['gongyi' , 'navlinkGy'], // 公益，其他导航
				['ly' , 'navlinkCs'], // 城市，其他导航
				['xinwenge' , 'navlinkXWG'] // 新闻哥，其他导航
			];

		for(var i=0, t=arr.length; i<t; i++){
			if( url.indexOf( arr[i][0] )>-1 ){
				document.getElementById(  arr[i][1] ).className = 'current';
				break;
			}
		}
		if( i>=t ){
			document.getElementById(  'navlinkIndex' ).className = 'current';
		}

	})();
</script><!--[if !IE]>|xGv00|43aa0273d06cee793fa8dd2cb01d0cba<![endif]-->
